<%= render 'shared/content_header', title: "Log in" %>

<%= bootstrap_form_for(:session, url: login_path, layout: :horizontal, label_col: "col-sm-2 col-sm-offset-1", control_col: "col-sm-6") do |f| %>
  <%= f.text_field :username %>
  <%= f.password_field :password %>
  <%= f.submit "Log in" %>
<% end %>
